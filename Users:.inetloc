  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "meta.definition.label.latex",
          "support.function.environment.latex",
          "keyword.control.begin.latex",
          "keyword.control.end.latex",
          "variable.parameter.function.latex",
          "meta.environment.latex",
          "support.function.section.tex",
          "support.function.general.tex",
          "support.function.ref.latex",
          "support.function.cite.latex"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "meta.environment.equation.latex",
          "meta.environment.equation.star.latex",
          "meta.environment.align.latex",
          "meta.environment.align.star.latex"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "meta.math.display.latex",
          "meta.math.inline.latex",
          "meta.environment.math.latex"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "text.tex.latex meta.math.inline.latex",
          "text.tex.latex meta.math.display.latex",
          "text.tex.latex meta.environment.math.latex",
          "text.tex.latex meta.environment.equation.latex",
          "text.tex.latex meta.environment.equation.star.latex",
          "text.tex.latex meta.environment.align.latex",
          "text.tex.latex meta.environment.align.star.latex"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "text.tex.latex string.other.math.tex",
          "text.tex.latex meta.embedded.block.math.tex",
          "text.tex.latex meta.math.block.latex",
          "text.tex.latex meta.math.display.latex",
          "text.tex.latex meta.math.inline.latex",
          "text.tex.latex meta.environment.math.latex",
          "text.tex.latex meta.environment.equation.latex",
          "text.tex.latex meta.environment.equation.star.latex",
          "text.tex.latex meta.environment.align.latex",
          "text.tex.latex meta.environment.align.star.latex",
          "text.tex.latex meta.environment.gather.latex",
          "text.tex.latex meta.environment.multline.latex",
          "text.tex.latex meta.environment.flalign.latex"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "meta.math.block.latex",
          "meta.math.display.latex",
          "string.other.math.block.tex",
          "meta.embedded.block.math.tex",
          "meta.math.block.markdown",
          "markup.raw.block.math.markdown",
          "text.tex.latex meta.math.block.latex",
          "text.tex.latex meta.math.display.latex"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "meta.math.inline.latex",
          "text.tex.latex meta.math.inline.latex",
          "string.other.math.tex",
          "text.tex.latex string.other.math.tex",
          "meta.embedded.inline.math.tex",
          "meta.embedded.math.inline.tex",
          "punctuation.definition.math.begin.latex",
          "punctuation.definition.math.end.latex",
          "punctuation.definition.math.begin.tex",
          "punctuation.definition.math.end.tex",
          "constant.other.math.tex",
          "constant.character.math.tex",
          "keyword.operator.math.tex",
          "variable.other.math.tex",
          "support.function.general.latex",
          "support.function.general.tex",
          "meta.math.inline.markdown",
          "markup.inline.raw.math.markdown"
        ],
        "settings": { "fontStyle": "italic" }
      },
      {
        "scope": [
          "meta.math",
          "meta.formula",
          "source.latex meta.math",
          "text.tex.latex meta.math",
          "text.tex meta.math",
          "source.tex meta.math",
          "meta.inline.math",
          "string.other.math",
          "string.quoted.other.math",
          "markup.inline.raw.math",
          "meta.embedded.math",
          "meta.region.math"
        ],
        "settings": { "fontStyle": "italic" }
      }
    ]
  },

