{
  "latex-workshop.showContextMenu": true,
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.message.error.show": false,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.latex.tools": [
  {
      "name": "latexmk-xelatex",
      "command": "latexmk",
      "args": [
        "-xelatex",
        "-latexoption=-synctex=1",
        "-latexoption=-interaction=nonstopmode",
        "-latexoption=-file-line-error",
        "-latexoption=-halt-on-error",
        "-silent",
        "-outdir=%OUTDIR%",
        "%DOCFILE%"
      ]
    },
  {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=batchmode",
        "-file-line-error",
        "-halt-on-error",
        "%DOCFILE%"
      ]
    },
    
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-halt-on-error",
        "%DOCFILE%"
      ]
    },
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-latexoption=-synctex=1",
        "-latexoption=-interaction=nonstopmode",
        "-latexoption=-file-line-error",
        "-latexoption=-halt-on-error",
        "-silent",
        "-outdir=%OUTDIR%",
        "%DOCFILE%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "latexmk-verbose",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-latexoption=-synctex=1",
        "-latexoption=-interaction=nonstopmode",
        "-latexoption=-file-line-error",
        "-latexoption=-halt-on-error",
        "-outdir=%OUTDIR%",
        "%DOCFILE%"
      ]
    },
    {
      "name": "latexmk-xelatex-verbose",
      "command": "latexmk",
      "args": [
        "-xelatex",
        "-latexoption=-synctex=1",
        "-latexoption=-interaction=nonstopmode",
        "-latexoption=-file-line-error",
        "-latexoption=-halt-on-error",
        "-outdir=%OUTDIR%",
        "%DOCFILE%"
      ]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "LaTeXmk",
      "tools": ["latexmk"]
    },
    {
      "name": "LaTeXmk (XeLaTeX)",
      "tools": ["latexmk-xelatex"]
    },
    {
      "name": "PDFLaTeX",
      "tools": ["pdflatex"]
    },
    {
      "name": "XeLaTeX",
      "tools": ["xelatex"]
    },
    {
      "name": "BibTeX",
      "tools": ["bibtex"]
    },
    {
      "name": "xelatex -> bibtex -> xelatex*2",
      "tools": ["xelatex", "bibtex", "xelatex", "xelatex"]
    },
    {
      "name": "pdflatex -> bibtex -> pdflatex*2",
      "tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
    },
    {
      "name": "LaTeXmk (verbose)",
      "tools": ["latexmk-verbose"]
    },
    {
      "name": "LaTeXmk (XeLaTeX, verbose)",
      "tools": ["latexmk-xelatex-verbose"]
    }
  ],
